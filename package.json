{
  "name": "foxglove-maplibre",
  "displayName": "foxglove-maplibre",
  "description": "Map panel using MapLibre",
  "publisher": "Kaliatech",
  "homepage": "https://github.com/kaliatech/foxglove-maplibre",
  "version": "0.0.1",
  "license": "MIT",
  "main": "./dist/extension.js",
  "keywords": [],
  "scripts": {
    "build": "foxglove-extension build",
    "dev": "nodemon --watch src --ext css,ts,tsx --exec \"npm run local-install\"",
    "format": "prettier --write .",
    "foxglove:prepublish": "foxglove-extension build --mode production",
    "lint:ci": "eslint --report-unused-disable-directives .",
    "lint": "eslint --report-unused-disable-directives --fix .",
    "local-install": "foxglove-extension install",
    "package": "foxglove-extension package",
    "pretest": "foxglove-extension pretest"
  },
  "devDependencies": {
    "@foxglove/extension": "^2.14.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "create-foxglove-extension": "^1.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "typescript": "^5.6.2"
  }
}
